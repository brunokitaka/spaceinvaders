.CODE 
MOV AX, @DATA
MOV DS, AX
MOV ES, AX

MOV AX, 13H
INT 10h

MOV AX, 1300H
MOV BX, 2			;COR
MOV DH, 2 	        ;LINHA
MOV DL, CENTRASPACE ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
MOV CX, LENESCAPE
LEA BP, SPACE
INT 10h

MOV AX, 1300H
MOV BX, 2            ;COR
MOV DH, 20 	  		  ;LINHA
MOV DL, CENTRAPRESS   ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
MOV CX, LENPRESS
LEA BP, PRESS
INT 10h

MOV AX, 1300H
MOV BX, 2            ;COR
MOV DH, 5 	  		  ;LINHA
MOV DL, CENFEITO   		  ;COLUNA
MOV CX, LENFEITO
LEA BP, FEITO
INT 10h



PRINTALIEN:
	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 8 	  		  ;LINHA
	MOV DL, CEN1       	  ;COLUNA
	MOV CX, 13
	LEA BP, ALIEN1
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 9 	  		  ;LINHA
	MOV DL, CEN2             ;COLUNA
	MOV CX, 13
	LEA BP, ALIEN2
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 10 	  		  ;LINHA
	MOV DL, CEN3       
	MOV CX, 13
	LEA BP, ALIEN3
	INT 10h
	
	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 11 	  		  ;LINHA
	MOV DL, CEN4       
	MOV CX, 13
	LEA BP, ALIEN4
	INT 10h
	
	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 12 	  		  ;LINHA
	MOV DL, CEN5       
	MOV CX, 13
	LEA BP, ALIEN5
	INT 10h
	
	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 13 	  		  ;LINHA
	MOV DL, CEN6       
	MOV CX, 13
	LEA BP, ALIEN6
	INT 10h
	
	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 14 	  		  ;LINHA
	MOV DL, CEN7       
	MOV CX, 13
	LEA BP, ALIEN7
	INT 10h
	
	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 15 	  		  ;LINHA
	MOV DL, CEN8       
	MOV CX, 13
	LEA BP, ALIEN8
	INT 10h

	MOV AH, 07h ;pressione qualquer tecla
	INT 21h

	CALL CLS

PRINTAMENU:

	MOV AX, @DATA
	MOV DS, AX
	MOV ES, AX

	MOV AX, 13H			;MODO DE VIDEO
	INT 10h
	
	MOV AX, 1300H		;PRINTA MSG DE SCORE TABLE
	MOV BX, 2H
	MOV DH, 5
	MOV DL, CENMSGPTS
	MOV CX, LENMSGPTS
	LEA BP, MSGPTS
	INT 10H
		MOV AX, 1300H		;VALOR DO PRIMEIOR ALIEN
	MOV BX, 2H
	MOV DH, 16
	MOV DL, CENPTS1
	MOV CX, LENPTS1
	LEA BP, PTS1
	INT 10H
	 
	MOV AX, 1300H			;VALOR DO SEGUNDO ALIEN
	MOV BX, 2H
	MOV DH, 13
	MOV DL, CENPTS2
	MOV CX, LENPTS2
	LEA BP, PTS2
	INT 10H
	
	MOV AX, 1300H			;VAlOR DO TERCEIRO ALIEN
	MOV BX, 2H	
	MOV DH, 10
	MOV DL, CENPTS3
	MOV CX, LENPTS3
	LEA BP, PTS3
	INT 10H
	
	MOV AX, 1300H
	MOV BX, 2             ;COR
	MOV DH, 20 	  		  ;LINHA
	MOV DL, CENTRAPRESS   ;COLUNA
	MOV CX, LENPRESS
	LEA BP, PRESS
	INT 10h
	
	PRINTCOISAS	ALIEN11, 15, 105, 80, 16, 128		;COR, X, Y, LARGURA E TOTAL
	PRINTCOISAS	ALIEN22, 15, 105, 104, 16, 128
	PRINTCOISAS	ALIEN33, 15, 105, 128, 16, 128

	 
	MOV AH, 07h ;pressione qualquer tecla
	INT 21H	 
	
	CALL CLS
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; INICIO JOGO
	
	
	
	MOV NAVEX, 50
	
	

MAINLOOP:

	MOV BX, NAVEX
	PRINTNAVE   NAVE, 2, BX, 190, 16, 64
	PRINTCOISAS SCORE, 15, 0, 0, 26, 156 
	
	; CALL VERIFICABARREIRA
	
	CALL PRINTABARREIRAS
	
	PRINTCOISAS LIVES, 15, 260, 1, 26, 156
	CMP COUNTLIVES,0
	JE CALL0
	CMP COUNTLIVES,1
	JE CALL1
	CMP COUNTLIVES,2
	JE CALL2
	CMP COUNTLIVES,3
	JE CALL3
	CMP COUNTLIVES,4
	JE CALLFIM
	
	CALL0: CALL PRINTLIVE3
	JMP NCST
	CALL1: CALL PRINTLIVE2
	JMP NCST
	CALL2: CALL PRINTLIVE1
	JMP NCST
	CALL3: CALL PRINTLIVE0
	JMP NCST
	CALLFIM: CALL PERDEUP
	
	NCST:
	CMP VERIFICAMOV, 0
	JE PRINTJOGO1
	CALL PRINTJOGO2
	JMP NXXT
	
	PRINTJOGO1:
	CALL PRINTJOGO     ;PRINTA TODOS ALIENS
	
	NXXT:
	PRINTCOISAS LIVES, 15, 260, 1, 26, 156
	MOV AH,01h
	INT 16h
	JZ NXT4
	
	MOV AH, 07h
	INT 21h
	
	CMP AL, 20h        ;verifica se é spacebar
	JE PTTIRO          ;se sim, pula para o tiro
	JMP NXT            ;se nao, segue
	
	PTTIRO:
	SUB SCOR, 5
	CMP VERIFICATIRO, 1
	JE PTVERIFICA	
	CALL TIROP   ;procedimento do tiro
	MOV VERIFICATIRO, 1

	 
	NXT:
	CMP AL, 61h         ;verifica se é 'a'
	JE NVES             ;se sim, pula para movimencao para esquerda
	JMP NXT3            ;se nao, segue
	
	NVES: SUB NAVEX, 8  ;movimentacao p/ esquerda, 8 px para esquerda
	CALL PRINTANAVEE
	
	NXT3:
	CMP AL, 64h         ;verifica se é 'd'
	JE NVDI				;se sim, pula para movimencao para esquerda
	JMP NXT4            ;se nao, segue
	
	NVDI: ADD NAVEX, 8  ;movimentacao p/ esquerda, 8 px para esquerda
	CALL PRINTANAVED

	NXT4:
	; MOV AH, 07h ;pressione qualquer tecla
	; INT 21h	
	CMP VERIFICATIRO, 1
	JE PTVERIFICA
	JMP NXT7
	
	PTVERIFICA:
	CALL ATUALIZATIRO
	CALL VERIFICAHIT
	
	NXT7:	
	
	
	CMP AL, 30h          
	JE PRINTFIM
	
	MOV ALCOUNT, 1
			
	VERIFICA_FINAL:		
	CMP ALCOUNT, 0    ;se nao houver aliens
	JE PRINTFIM      ;pula para print do fim
;-----------------------------------------------------------
	JMP MAINLOOP	  ;se nao, atualiza
;-----------------END MAIN LOOP-------------------------------------

PONTTE: JMP PRINTAMENU	
PRINTFIM:
CALL CLS
MOV AX, @DATA
MOV ES, AX
	
CALL PRINTGANHOU

MOV AH, 0h ;pressione qualquer tecla
INT 16h

CMP AL, 115d
JE PONTTE
CMP AL, 110d
JE FIMNAL

FIMNAL:
CALL CLS

MOV AH, 4CH
INT 21H

PRINTAMAT PROC
		MOV	LADO_X,0
		MOV	LADO_Y,0
		MOV	COUNT,0

		INICIO:
			MOV	CX, X_INICIO
			ADD	CX, LADO_X
			INC	LADO_X
			MOV	DX, Y_INICIO
			ADD	DX, LADO_Y

			MOV	BL, [SI]   ;joga posicao da matriz

			CMP	BL, 0      ;se for 0
			JE	PRINT0     ;printa 
	
			MOV	AH, 0CH
			INT	10H

		PRINT0:	
				INC	SI      ;posicao + 1
				INC	COUNT   ;count++

				MOV	BX, LARGURAMAT   ;verifica se atingiu o limite
				CMP	LADO_X, BX
				JL	NEXT        ;se nao continua

				MOV	LADO_X, 0   ;se for, volta
				INC	LADO_Y      ;aumenta linha

		NEXT:	
				MOV	BX, TOTALMAT   ;verifica se ja foi printado tudo
				CMP	COUNT, BX
				JL	INICIO        ;se nao, volta ao inicio

				RET

PRINTAMAT ENDP
	
CLS PROC
	PUSH	AX					;Salva 
	PUSH	BX					
	PUSH	CX
	PUSH	DX
	
	MOV		AX,0A000H
	MOV		ES,AX
	XOR		DI,DI
	XOR		AX,AX
	MOV		CX,32000
	CLD
	REP		STOSW

	POP		DX					;OPEN
	POP		CX
	POP		BX
	POP		AX
			
	RET
CLS ENDP

PRINTJOGO PROC
	MOV ALIENY, 30                        ;APAGA OS ALIENS
