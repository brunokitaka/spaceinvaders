	RET
	
	MOVESQ:
	ADD COUNTY, 2
	MOV VERIFICAMOV, 1
	CALL CLS
	RET
PRINTJOGO ENDP

; MOVEALIENDIR PROC

	; PUSH	AX
	; PUSH	BX
	; PUSH	CX
	; PUSH	DX
			
	; INC		NAVEX			
			
	; POP AX
	; POP	BX
	; POP	CX
	; POP	DX

	; RET
; MOVEALIENDIR ENDP

TIROP PROC
	PUSH BX
	PUSH NAVEX
	PUSH CX
	
	MOV BX, NAVEX  ;posicao nave
	ADD BX, 7      ;alinha tiro com bico da nave
	MOV NAVEX, BX
	MOV CX, 186
	PRINTCOISAS TIRO, 15, BX, CX, 2, 6

	LOOPDOTIRO:
			
		PUSH CX  ;SALVA
		CALL DELAY
		POP CX   ;OPEN       
		
		MOV BX, NAVEX
		PRINTCOISAS TIRO, 0, BX, CX, 2, 6

		PUSH CX  ;SALVA
		CALL DELAY
		POP CX   ;OPEN 	

		MOV BX, NAVEX
		SUB CX, 3
		PRINTCOISAS TIRO, 15, BX, CX, 2, 6
	
		
		JMP NXT2
		PNT1: JMP LOOPDOTIRO
		
		NXT2:
		CMP CX, 0
		JNZ PNT1
		
		MOV BX, NAVEX
		PRINTCOISAS TIRO, 0, BX, 0, 2, 6
		
		POP CX
		POP NAVEX		
	    POP BX
		RET
TIROP ENDP

DELAY PROC

	PUSH	AX
	PUSH	BX
	PUSH	CX
	PUSH	DX
	
	
 	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
 	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H

	POP AX
	POP	BX
	POP	CX
	POP	DX
	
	RET
DELAY ENDP

PRINTANAVEE PROC                            ;movimenta nave esquerda
	PRINTNAVE   NAVE, 0, 50, 190, 16, 64
	MOV BX, NAVEX
	ADD BX, 8
	PRINTNAVE   NAVE, 0, BX, 190, 16, 64
	MOV BX, NAVEX
	PRINTNAVE   NAVE, 2, BX, 190, 16, 64	
	
	RET
PRINTANAVEE ENDP

PRINTANAVED PROC                            ;movimenta nave direita
	PRINTNAVE   NAVE, 0, 50, 190, 16, 64
	MOV BX, NAVEX
	SUB BX, 8
	PRINTNAVE   NAVE, 0, BX, 190, 16, 64
	MOV BX, NAVEX
	PRINTNAVE   NAVE, 2, BX, 190, 16, 64	
	
	RET
PRINTANAVED ENDP

KILLIEN PROC
RET

KILLIEN ENDP

