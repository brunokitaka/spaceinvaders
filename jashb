	MOV CX, ALIENY
	ADD CX, COUNTY
	MOV ALIENX, 270
	MOV BX, ALIENX
	ADD BX, COUNTX
	PRINTCOISAS	ALIEN33, 15, BX, CX, 16, 128
	
	CMP COUNTX, 0
	JE MOVESQQ
	
	DEC COUNTX
	RET
	
	MOVESQQ:
	ADD COUNTY, 2
	MOV VERIFICAMOV, 0
	CMP COUNTY, 60
	JE LIVEN
	CALL CLS
	RET
	
	LIVEN:
	INC BARREIRACOUNT
	INC COUNTLIVES
	MOV COUNTY, 0
	CALL CLS
	RET
PRINTJOGO2 ENDP

PRINTLIVE3 PROC
	PRINTCOISAS CORACAO, 12, 289, 0, 9, 72
	PRINTCOISAS CORACAO, 12, 299, 0, 9, 72
	PRINTCOISAS CORACAO, 12, 309, 0, 9, 72
	RET
PRINTLIVE3 ENDP

PRINTLIVE2 PROC
	PRINTCOISAS CORACAO, 12, 289, 0, 9, 72
	PRINTCOISAS CORACAO, 12, 299, 0, 9, 72
	RET
PRINTLIVE2 ENDP

PRINTLIVE1 PROC
	PRINTCOISAS CORACAO, 12, 289, 0, 9, 72
	RET
PRINTLIVE1 ENDP

PRINTLIVE0 PROC
	
	RET
PRINTLIVE0 ENDP

PERDEUP PROC
	CALL CLS
	MOV AX, @DATA
	MOV ES, AX
		
	MOV AX, 1300H		;mensagem perdeu
	MOV BX, 2			;COR
	MOV DH, 10 	        ;LINHA
	MOV DL, CENPERDEU 	;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
	MOV CX, LENPERDEU
	LEA BP, PERDEU
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 20 	  		  ;LINHA
	MOV DL, CENTRAPRESS   ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
	MOV CX, LENPRESS
	LEA BP, PRESS
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 24 	  		  ;LINHA
	MOV DL, CENFEITO   		  ;COLUNA
	MOV CX, LENFEITO
	LEA BP, FEITO
	INT 10h

	MOV AH, 07h ;pressione qualquer tecla
	INT 21h

	MOV AH, 4CH
	INT 21H
PERDEUP ENDP

; VERIFICABARREIRA PROC

	; CMP COUNTY, 60
	; JE DEGRADA
	; RET

	; DEGRADA:
	; INC BARREIRACOUNT
	
	; RET
; VERIFICABARREIRA ENDP

PRINTABARREIRAS PROC
	CMP BARREIRACOUNT, 0
	JE DEGPT1
	CMP BARREIRACOUNT, 1
	JE DEGPT2
	CMP BARREIRACOUNT, 2
	JE DEGPT3
	CMP BARREIRACOUNT, 3
	JE DEGPT4
	CALL PERDEUP
	
	DEGPT1: JMP DEGRADA1
	DEGPT2: JMP DEGRADA2
	DEGPT3: JMP DEGRADA3
	DEGPT4: JMP DEGRADA4
	 
	DEGRADA1:
		PRINTCOISAS	BARR, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR, 2, 260, 170, 20, 160	
	DEGRADA2:	
		PRINTCOISAS	BARR2, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 260, 170, 20, 160	
	DEGRADA3:	
		PRINTCOISAS	BARR3, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 260, 170, 20, 160		
	DEGRADA4:	
		PRINTCOISAS	BARR4, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 260, 170, 20, 160	
RET
PRINTABARREIRAS ENDP

	

END
