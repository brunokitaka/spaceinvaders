PERDEUP PROC
	CALL CLS
	MOV AX, @DATA
	MOV ES, AX
		
	MOV AX, 1300H		;mensagem perdeu
	MOV BX, 2			;COR
	MOV DH, 10 	        ;LINHA
	MOV DL, CENPERDEU 	;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
	MOV CX, LENPERDEU
	LEA BP, PERDEU
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 20 	  		  ;LINHA
	MOV DL, CENTRAPRESS   ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
	MOV CX, LENPRESS
	LEA BP, PRESS
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 24 	  		  ;LINHA
	MOV DL, CENFEITO   		  ;COLUNA
	MOV CX, LENFEITO
	LEA BP, FEITO
	INT 10h

	MOV AH, 07h ;pressione qualquer tecla
	INT 21h

	MOV AH, 4CH
	INT 21H
PERDEUP ENDP

; VERIFICABARREIRA PROC

	; CMP COUNTY, 60
	; JE DEGRADA
	; RET

	; DEGRADA:
	; INC BARREIRACOUNT
	
	; RET
; VERIFICABARREIRA ENDP

PRINTABARREIRAS PROC
	CMP BARREIRA1COUNT, 0
	JE NORMAL
	CMP BARREIRA1COUNT, 1
	JE DEGPT2
	CMP BARREIRA1COUNT, 2
	JE DEGPT3
	CMP BARREIRA1COUNT, 3
	JE DEGPT4
	CMP BARREIRA1COUNT, 4
	JE DEGPT5
	CMP BARREIRA1COUNT, 5
	JE DEGPT6
	JMP NADA1
	
	
	NORMAL: JMP DEGRADA10
	DEGPT2: JMP DEGRADA11
	DEGPT3: JMP DEGRADA12
	DEGPT4: JMP DEGRADA13
	DEGPT5: JMP DEGRADA14
	DEGPT6: JMP NADA1
	 
	 
	DEGRADA10: PRINTCOISAS	BARR, 2, 40, 170, 20, 160
				JMP NADA1	
	DEGRADA11: PRINTCOISAS	BARR2, 2, 40, 170, 20, 160
				JMP NADA1
	DEGRADA12: PRINTCOISAS	BARR3, 2, 40, 170, 20, 160
				JMP NADA1
	DEGRADA13: PRINTCOISAS	BARR4, 2, 40, 170, 20, 160
				JMP NADA1
	DEGRADA14: PRINTCOISAS	BARR5, 2, 40, 170, 20, 160
	NADA1:
	
	 
	CMP BARREIRA2COUNT, 0
	JE NORMAL2
	CMP BARREIRA2COUNT, 1
	JE DEGPT22
	CMP BARREIRA2COUNT, 2
	JE DEGPT32
	CMP BARREIRA2COUNT, 3
	JE DEGPT42
	CMP BARREIRA2COUNT, 4
	JE DEGPT52
	CMP BARREIRA2COUNT, 5
	JE DEGPT62
	JMP NADA2
	
	NORMAL2: JMP DEGRADA20
	DEGPT22: JMP DEGRADA21
	DEGPT32: JMP DEGRADA22
	DEGPT42: JMP DEGRADA23
	DEGPT52: JMP DEGRADA24
	DEGPT62: JMP NADA2
	
	DEGRADA20: PRINTCOISAS	BARR, 2, 40, 170, 20, 160
				JMP NADA2	
	DEGRADA21: PRINTCOISAS	BARR2, 2, 40, 170, 20, 160
				JMP NADA2
	DEGRADA22: PRINTCOISAS	BARR3, 2, 40, 170, 20, 160
				JMP NADA2
	DEGRADA23: PRINTCOISAS	BARR4, 2, 40, 170, 20, 160
				JMP NADA2
	DEGRADA24: PRINTCOISAS	BARR5, 2, 40, 170, 20, 160
	NADA2:
	
	
	
	
	 
	 
	DEGRADA1:
		PRINTCOISAS	BARR, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR, 2, 260, 170, 20, 160	
	DEGRADA2:	
		PRINTCOISAS	BARR2, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 260, 170, 20, 160	
	DEGRADA3:	
		PRINTCOISAS	BARR3, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 260, 170, 20, 160		
	DEGRADA4:	
		PRINTCOISAS	BARR4, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 260, 170, 20, 160	
RET
PRINTABARREIRAS ENDP

ATUALIZATIRO PROC		
		PUSH CX  ;SALVA
		CALL DELAY
		POP CX   ;OPEN       
		
		MOV BX, SALVAX
		MOV CX, TIROY
		PRINTCOISAS TIRO, 0, BX, CX, 2, 6

		PUSH CX  ;SALVA
    	CALL DELAY
    	POP CX   ;OPEN 	
    
    	
    	SUB TIROY, 6
		MOV DX, TIROY
		MOV SALVAY, DX
		MOV CX, TIROY
		MOV BX, SALVAX
    	PRINTCOISAS TIRO, 15, BX, CX, 2, 6
    	
    	NXT2:
    	CMP TIROY, 0
    	JE FIMTIRO
		RET
    	
		FIMTIRO:
    	MOV BX, SALVAX
    	PRINTCOISAS TIRO, 0, BX, 0, 2, 6
		MOV VERIFICATIRO, 0
		


	RET
ATUALIZATIRO ENDP

PRINTGANHOU PROC
MOV AX, 1300H		;mensagem ganhou
MOV BX, 2			;COR
MOV DH, 10 	        ;LINHA
MOV DL, CENGANHOU 	;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
MOV CX, LENGANHOU
LEA BP, GANHOU
INT 10h

; MOV AX, 1300H
; MOV BX, 15            ;COR
; MOV DH, 20 	  		  ;LINHA
; MOV DL, CENTRAPRESS   ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
; MOV CX, LENPRESS
; LEA BP, PRESS
; INT 10h

MOV AX, 1300H
MOV BX, 2            ;COR
MOV DH, 17 	  		  ;LINHA
MOV DL, CENJOGAR   		  ;COLUNA
MOV CX, LENJOGAR
LEA BP, JOGAR
INT 10h

MOV AX, 1300H
MOV BX, 2            ;COR
MOV DH, 24	  		  ;LINHA
MOV DL, CENFEITO   		  ;COLUNA
MOV CX, LENFEITO
LEA BP, FEITO
INT 10h

RET
PRINTGANHOU ENDP

VERIFICAHIT PROC

	CMP SALVAX, 40
	JGE COMPX40
	PT110:
	CMP SALVAX, 110
	JE COMPX110
	PT190:
	CMP SALVAX, 190
	JE COMPX190
	PT260:
	CMP SALVAX, 260
	JE COMPX260
		
		
		COMPX40:CMP SALVAX, 60
				JLE COMPY40
				JMP PT110
		COMPX110:CMP SALVAX, 130
				 JLE COMPY110
				 JMP PT190	
		COMPX190:CMP SALVAX, 210
				 JLE COMPY190
				 JMP PT260
		COMPX260:CMP SALVAX, 280
				 JLE COMPY260
				 JMP SEGUE					 
		
		COMPY40: CMP SALVAY, 170
			     JE  HITBARR1
				 JMP SEGUE
		COMPY110:CMP SALVAY, 170
			     JE  HITBARR2
				 JMP SEGUE
		COMPY190:CMP SALVAY, 170
			     JE  HITBARR3
				 JMP SEGUE
		COMPY260:CMP SALVAY, 170
			     JE  HITBARR4
                 JMP SEGUE

		HITBARR1:
		INC BARREIRA1COUNT
		MOV VERIFICATIRO, 0
		RET
		HITBARR2:
		INC BARREIRA2COUNT
		MOV VERIFICATIRO, 0
		RET
		HITBARR3:
		INC BARREIRA3COUNT
		MOV VERIFICATIRO, 0
		RET
		HITBARR4:
		INC BARREIRA4COUNT
		MOV VERIFICATIRO, 0
		RET
	

	SEGUE:
	MOV BX, XENS1
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 1
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 2
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 3
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 4
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 5
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 6
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 7
	CMP SALVAX, BX
	JE PONTECOL1
	JMP NECST1
									PONTECOL1: JMP COLUNA1
									NECST1:
	MOV BX, XENS1
	ADD BX, 8
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 9
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 10
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 11
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 12
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 13
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 14
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 15
	CMP SALVAX, BX
	JE PONTECOL1
	
	MOV BX, XENS2
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 1
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 2
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 3
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 4
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 5
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 6
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 7
	CMP SALVAX, BX
	JE PONTECOL2
	JMP NECST2
									PONTECOL2: JMP COLUNA2
									NECST2:
	MOV BX, XENS2
	ADD BX, 8
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 9
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 10
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 11
	CMP SALVAX, BX
	JE PONTECOL2
	MOV BX, XENS2
	ADD BX, 12
