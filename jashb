	RET
	
	MOVESQQ:
	ADD COUNTY, 2
	MOV VERIFICAMOV, 0
	CMP COUNTY, 60
	JE LIVEN
	CALL CLS
	RET
	
	LIVEN:
	INC BARREIRACOUNT
	INC COUNTLIVES
	MOV COUNTY, 0
	CALL CLS
	RET
