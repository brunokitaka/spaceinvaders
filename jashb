			DB	0,0,1,1,1,1,1,0,0
			DB  0,0,0,1,1,1,0,0,0
			DB	0,0,0,0,1,0,0,0,0		
			
	ALIENX DW 0
	ALIENY DW 0
	TALIENS DW 0
